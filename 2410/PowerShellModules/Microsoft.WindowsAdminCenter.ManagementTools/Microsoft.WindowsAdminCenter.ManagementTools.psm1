<#

.DESCRIPTION
Support Enter-WACPSSession to specified computer as specified username. Password will be promoted.
- Enabled by adding "%ProgramFiles%\Windows Admin Center\PowerShell\Modules" to $env:PSModulePath

#>

<#
.SYNOPSIS
    Remote logon with a PowerShell session to a computer.

.DESCRIPTION
    Remote logon with a PowerShell session to a computer with user name and password.
    It overrides Get-Credential cmdlet to suppress native credential UI.

.PARAMETER Computer
    The remote host.

.PARAMETER UserName
    User name for PowerShell session.

.PARAMETER UseHttps
    If true, use HTTPS to connect to the remote host.

.PARAMETER PowerShellSessionConfiguration
    Specifies the name of the session configuration to use for PowerShell remote session.

.PARAMETER UserNameLabel
    Label for user name prompt.

.PARAMETER PromptLabel
    Label for password prompt.

.PARAMETER Message
    Message to display before prompting for user name and password.

.EXAMPLE
    Enter-WACPSSession -Computer "contoso.corp.net" -UserName "user" -UseHttps $true -UserNameLabel "Username" -PromptLabel "Password" -Message "Connecting ..."
#>
Function Enter-WACPSSession {
  Param(
    [Parameter(Mandatory = $True, Position = 0)]
    [string]$Computer,
    [Parameter(Mandatory = $True, Position = 1)]
    [string]$UserName,
    [Parameter(Mandatory = $False, Position = 2)]
    [bool]$UseHttps = $False,
    [Parameter(Mandatory = $False, Position = 3)]
    [string]$PowerShellSessionConfiguration = 'default',
    [Parameter(Mandatory = $False, Position = 4)]
    [string]$UserNameLabel = "Username",
    [Parameter(Mandatory = $False, Position = 5)]
    [string]$PromptLabel = "Password",
    [Parameter(Mandatory = $False, Position = 6)]
    [string]$Message = "Connecting ..."
  )

    $GetCredentialScript = {
        # passing and remember labels.
        $Global:__MsftSmeUserNameLabel = $args[0]
        $Global:__MsftSmePromptLabel = $args[1]

        <#

        .SYNOPSIS
        Gets a credential object based on a user name and password.

        .DESCRIPTION
        Gets a credential object based on a user name and password.
        (Use Microsoft.PowerShell.Security\Get-Credential to use native Get-Credential CmdLet)

        #>
        Function Get-Credential {
            [CmdletBinding(DefaultParametersetName = 'Prompt')]
            Param(
                [Parameter(Mandatory = $True, ParameterSetName = 'Creds')]
                [PSCredential]$Credential,
                [Parameter(Mandatory = $False, ParameterSetName = 'Prompt')]
                [string]$UserName = $null,
                [Parameter(Mandatory = $False, ParameterSetName = 'Prompt')]
                [string]$Message = $null
            )

            Set-PSReadLineOption -HistorySaveStyle SaveNothing

            if ($Credential) {
                return Microsoft.PowerShell.Security\Get-Credential -Credential $Credential
            }

            if ($Message) {
                Microsoft.PowerShell.Utility\Write-Host $Message
            }

            if ($UserName) {
                $UserName = $UserName
                Microsoft.PowerShell.Utility\Write-Host $UserName
            }
            else {
                $UserName = Microsoft.PowerShell.Utility\Read-Host -Prompt $Global:__MsftSmeUserNameLabel
            }

            $password = Microsoft.PowerShell.Utility\Read-Host -AsSecureString -Prompt $Global:__MsftSmePromptLabel
            $Credential = New-Object System.Management.Automation.PSCredential $UserName, $password
            return $Credential
        }
    }

    function GetUserDomain($userName) {
		if ($userName.Contains("\\")) {
            $domain = $userName -Split "\\" | Select-Object -First 1
            if ($domain -eq ".") {
				$domain = $Env:UserDomain
			}

            return $domain
        } else {
            return $Env:UserDomain
        }
    }

    function GetUserName($userName) {
        if ($userName.Contains("\\")) {
			return $userName -Split "\\" | Select-Object -Last 1
		} else {
			return $userName
		}
    }

    function IsSameUser($userName) {
        $currentIdentity = [System.Security.Principal.WindowsIdentity]::GetCurrent().Name
        $currentUserDomain = GetUserDomain $currentIdentity
        $currentUserName = GetUserName $currentIdentity
        
        $givenUserDomain = GetUserDomain $userName
        $givenUserName = GetUserName $userName

        return $currentUserName.ToLower() -eq $givenUserName.ToLower() -and $currentUserDomain.ToLower() -eq $givenUserDomain.ToLower()
    }

    function CreateSession($userName, $password) {
        if ([System.string]::IsNullOrEmpty($userName)) {
            throw "Missing username!"
        }

        $PSSessionConfigurationName = if ($PowerShellSessionConfiguration -eq 'default') { $null } else { $PowerShellSessionConfiguration }
        Set-PSReadLineOption -HistorySaveStyle SaveNothing

        if ($null -ne $password) {
            $Credential = Microsoft.PowerShell.Utility\New-Object -TypeName System.Management.Automation.PSCredential -ArgumentList $userName, $password
            $Session = New-PSSession -ComputerName $Computer -UseSSL:$UseHttps -EnableNetworkAccess -Credential $Credential -ErrorAction Stop -ConfigurationName $PSSessionConfigurationName
        }
        elseif (IsSameUser $userName) {
            $Session = New-PSSession -ComputerName $Computer -UseSSL:$UseHttps -EnableNetworkAccess -ErrorAction Stop -ConfigurationName $PSSessionConfigurationName
        }
        else {
            throw "Missing password!"
        }

        Enter-PSSession -Session $session -ErrorAction Stop
        Invoke-Command -Session $session -ScriptBlock $GetCredentialScript -ArgumentList $UserNameLabel, $PromptLabel
    }

    function EnterSessionAskPassword($askUserName) {
        try {
            CreateSession $UserName $null
        }
        catch {
            foreach ($i in 0, 1, 2) {
                $enterSession = $false
                try {
                    if ($askUserName) {
                        $UserName = Microsoft.PowerShell.Utility\Read-Host -Prompt $UserNameLabel
                        if (($UserName.IndexOf("\") -lt 0) -and ($UserName.IndexOf("@") -lt 0)) {
                            $UserName = ".\$UserName";
                        }
                    }

                    $password = Microsoft.PowerShell.Utility\Read-Host -AsSecureString -Prompt $PromptLabel
                }
                finally {
                    if (($null -ne $UserName) -and ($null -ne $password)) {
                        $enterSession = $true
                    }
                }

                if ($enterSession) {
                    try {
                        CreateSession $UserName $password
                        break
                    }
                    catch {
                        Microsoft.PowerShell.Utility\Write-Host -ForegroundColor Yellow $_.Exception.Message
                    }
                }
            }
        }
    }

    Clear-Host

    function Global:Prompt {
        $LocalName = [System.Environment]::MachineName
        Microsoft.PowerShell.Utility\Write-Host -ForegroundColor Yellow -NoNewline "[GW $LocalName] $pwd>"; return " "
    }

    $askUserName = $false
    if ($UserName -eq "<NA>") {
        $UserName = $null
        $askUserName = $true
    }

    Microsoft.PowerShell.Utility\Write-Host -ForegroundColor Cyan $Message
    EnterSessionAskPassword $askUserName
}

